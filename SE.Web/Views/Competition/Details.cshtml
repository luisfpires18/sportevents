@model SE.Web.Models.Event

@{
    ViewBag.Title = "Event Details";
}

<h2>@ViewBag.Title</h2>

    <div>
        <h4><b>Name:</b> @Model.Name</h4>
        <h4><b>Local:</b> @Model.Local</h4>
        <h4><b>Date:</b> @Model.EventDate.ToShortDateString()</h4>
    </div>
<br /><br />
<a class="btn btn-default" asp-action="Index">Back to List</a>
<a class="btn btn-default" asp-action="Delete" asp-route-id="@Model.EventID">Delete</a>

